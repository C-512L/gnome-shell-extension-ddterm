{
    "problemMatcher": [
        {
            "owner": "gettext-stats",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "msgfmt .* --statistics .* (po/.*\\.po)$",
                    "file": 1
                },
                {
                    "regexp": "^((\\d+) translated messages?,.*)",
                    "message": 1
                }
            ]
        }
    ]
}
